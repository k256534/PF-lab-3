#include <iostream>
using namespace std;

int main() {
    int amount;

    cout << "Enter total purchase amount: ";
    cin >> amount;

    if (amount >= 5000) {
        cout << "20% discount" << endl;
    }
    else if (amount >= 3000) {
        cout << "10% discount" << endl;
    }
    else {
        cout << "No discount" << endl;
    }

    return 0;
}
