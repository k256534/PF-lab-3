
#include <iostream>
#include <string>
using namespace std;

int main() {
    string username; 
	int password ;
	
	cout << " Enter username"; 
	cin >> username;
	
	cout << " Enter password"; 
	cin >> password;
	
	if (username == "user" && password == 1233 ){
		cout << "connected successfully"<< endl;
	
	}
	
	else {
		cout << "connection failed"<<  endl;
	}
	
	return 0;
}
